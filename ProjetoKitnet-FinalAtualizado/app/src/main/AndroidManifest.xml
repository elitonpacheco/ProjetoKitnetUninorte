<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.elitonpacheco.projetokitnet">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/casa01"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/casa01"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginProjetoKitnet"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivityCadInquilino"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityCadKitnet"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityGerInquilino"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityGerKitnet"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityRecibo"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityGerPagamentos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityPendsPagamentos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityFinHistorico"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityGeraTiketManut"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityRelaTiketsAbertos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivityHist_Tikets"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity_Kitnet"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>